{
  "name": "open-orchestra/open-orchestra-newsletter-bundle",
  "type": "package",
  "description": "A bundle which provides a newsletter subscription management for Open Orchestra",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "OpenOrchestra community",
      "homepage": "https://github.com/open-orchestra/open-orchestra-newsletter-bundle/graphs/contributors"
    }
  ],
  "require": {
    "open-orchestra/open-orchestra-display-bundle": "~1.1.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "4.8.2",
    "psr/log": "1.0.0",
    "codeclimate/php-test-reporter": "dev-master",
    "open-orchestra/open-orchestra-base-bundle": "~1.1.0",
    "phake/phake": "2.1.0"
  },
  "config" : {
    "bin-dir" : "bin"
  },
  "suggest": {
    "open-orchestra/open-orchestra-model-bundle": "To use the mongo functionnality"
  },
  "autoload": {
    "psr-4": {
      "OpenOrchestra\\NewsletterAdminBundle\\": "NewsletterAdminBundle/",
      "OpenOrchestra\\NewsletterModelBundle\\": "NewsletterModelBundle/",
      "OpenOrchestra\\NewsletterBundle\\": "NewsletterBundle/",
      "OpenOrchestra\\Newsletter\\": "Newsletter/"
    }
  }
}
