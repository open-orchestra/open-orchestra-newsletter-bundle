  },
  "require-dev": {
    "phpunit/phpunit" : "4.8.2",
    "psr/log": "1.0.0",
    "codeclimate/php-test-reporter": "dev-master",
    "open-orchestra/open-orchestra-base-bundle": "dev-master",
